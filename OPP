# L35 | Composition | Has-A relationship
class Engine:

    def __init__(self, cc):
        self.capacity = cc

    def start(self):
        print("Engine started")

    def stop(self):
        print("Engine stopped")


class Car:

    def __init__(self, name, cc):
        self.name = name
        self.engine = Engine(cc)  # create an object

    def run(self):
      # print(self.Engine)
      self.engine.start()
      print("Car is running")

c1 = Car("BMW", 1000)
c1.run()

