# Method Overloading:
# Jokon akta Design class ar same namer method differnt Perameter thake
# Python Same name overloading support kore nah

class my_calculator:
  def product(self, num1, num2=None, num3=None):
    if num1 != None and num2 != None and num3 != None:
      print(num1 * num2 * num3)
    elif num1 != None and num2 != None:
      print(num1 * num2)
    else:
      print(1 * num1)

c1 = my_calculator()
c1.product(4)
c1.product(4, 5)
c1.product(4, 5, 6)

#=========================================================#

# Method Overloading:
# Jokon akta Design class ar same namer method differnt Perameter thake
# Python Same name overloading support kore nah

class my_calculator:
  
  def product(self, *num):
    sum = 1
    for x in num:
      sum = sum * x
    print("Product is: ", sum)

c1 = my_calculator()
c1.product(4)
c1.product(4, 5)
c1.product(4, 5, 6)

#===========================================================#

from multipledispatch import dispatch
class my_calculator:
  
  @dispatch(int, int)
  def product(self, a, b):
    print(a * b)

  @dispatch(int, int, int)
  def product(self, a, b, c):
    print(a * b * c)

  @dispatch(str, str)
  def product(self, a, b):
    print(int(a) * int(b))

  @dispatch(float, float, float)
  def product(self, a, b, c):
    print(a * b * c)


m1 = my_calculator()
m1.product(4, 5)
m1.product(4, 5, 6)
m1.product("4", "5")
m1.product(4.0, 5.0, 6.0)

