  def __init__(self, name, id):
    self.name = name
    self.id = id

  def __init__(self, name, id, code):
    self.name = name
    self.id = id
    self.code = code

a = my_class("Ahsan", 1)
a1 = my_class("Ahsan", 1, 3)

