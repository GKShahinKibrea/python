class Book:
  def __init__(self, book_name, author):
    self.book_name = book_name
    self.author = author
    self.price = 0

  def set_price(self, price):
    self.price = price

  def get_price(self):
    return self.price

  def details(self):
    print("Book Name:", self.book_name ,"\nAuthor:", self.author, "\nPrice:", self.price)

b1 = Book("Harry Potter", "shahin kibrea")
b1.set_price(123)
print(b1.get_price())
b1.details()

