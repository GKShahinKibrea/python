# Exercise | OOP | CSE111 Tracing | Lab Assignment-7


class A:
    temp = 4

    def __init__(self):
        self.sum = 0
        self.y = 0
        self.y = A.temp - 2  # 2
        self.sum = A.temp + 1  # 4 + 1 = 5
        A.temp -= 2  # 4-2=2

    def methodA(self, m, n):
        x = 0
        self.y = self.y + m + (A.temp)  # 2 + 1 + 2 = 5
        A.temp += 1  # 2 + 1 = 3
        x = x + 1 + n  # 0 + 1 + 2 = 3
        self.sum = self.sum + x + self.y  # 5 + 3 + 5 = 13
        print(x, self.y, self.sum)


class B(A):
    x = 0

    def __init__(self, b=None):
        super().__init__()
        self.sum = 0
        if b == None:
            self.y = A.temp + 3
            self.sum = 3 + A.temp + 2
            A.temp -= 2
        else:
            self.sum = b.sum
            B.x = b.x
            b.methodB(2, 3)

    def methodB(self, m, n):
        y = 0
        y = y + self.y
        B.x = self.y + 2 + A.temp
        self.methodA(B.x, y)
        self.sum = B.x + y + self.sum
        print(B.x, y, self.sum)


a1 = A()
b1 = B()
b2 = B(b1)
b1.methodA(1, 2)
b2.methodB(3, 2)

